<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER" style="-fx-spacing: 200;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ss.studysystem.controller.survey.survey_buttons_controller">
   <children>
      <Button fx:id="back_btn" mnemonicParsing="false" style="-fx-background-color: white; -fx-border-color: white;">
         <graphic>
            <ImageView fitHeight="27.0" fitWidth="22.0" nodeOrientation="RIGHT_TO_LEFT" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/img.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="next_btn" mnemonicParsing="false" style="-fx-border-color: white; -fx-background-color: white;">
         <graphic>
            <ImageView fitHeight="28.0" fitWidth="22.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/img.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</HBox>
