<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="45.0" prefWidth="152.0" style="-fx-border-color: black; -fx-border-radius: 2em; -fx-background-color: E6E8EB; -fx-background-radius: 2em;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ss.studysystem.controller.friends">
   <children>
      <ImageView fx:id="profile_img" fitHeight="28.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../image!/users-alt.png" />
         </image>
         <HBox.margin>
            <Insets left="10.0" top="10.0" />
         </HBox.margin>
      </ImageView>
      <Label fx:id="name" prefHeight="17.0" prefWidth="85.0" text="Placeholder">
         <HBox.margin>
            <Insets left="10.0" top="10.0" />
         </HBox.margin>
         <font>
            <Font size="13.0" />
         </font>
      </Label>
   </children>
</HBox>
