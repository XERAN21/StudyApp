<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.TextFlow?>

<VBox style="-fx-padding: 10; -fx-background-color: transparent;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ss.studysystem.controller.classroom.class_home">
   <children>
      <ScrollPane fitToHeight="true" fitToWidth="true" stylesheets="@../../css/scroll_round.css" VBox.vgrow="ALWAYS">
         <content>
            <VBox fx:id="home_view1" minHeight="-Infinity" minWidth="-Infinity" style="-fx-background-color: F3EEF8; -fx-background-radius: 1em; -fx-border-color: black; -fx-border-radius: 1em; -fx-padding: 15;">
               <children>
                  <TextFlow>
                     <children>
                        <Text fx:id="classname" strokeType="OUTSIDE" strokeWidth="0.0" text="Classroom #1">
                           <font>
                              <Font name="Monospaced Bold" size="18.0" />
                           </font>
                        </Text>
                     </children>
                  </TextFlow>
                  <VBox fx:id="home_root" alignment="TOP_CENTER" style="-fx-background-color: F3EEF8; -fx-background-radius: 1em; -fx-spacing: 15;" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
         </content>
      </ScrollPane>
   </children>
</VBox>
