<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<HBox prefWidth="200.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ss.studysystem.Fxml.classroom.Assignment">
    <children>
        <ImageView fitHeight="37.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../images/Notice%20Icon.png" />
            </image>
        </ImageView>
        <VBox>
            <children>
                <Button mnemonicParsing="false" style="-fx-background-color: transparent;" text="Notice" underline="true" />
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Date" />
            </children>
        </VBox>
    </children>
</HBox>
